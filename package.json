{
  "name": "codebase-js",
  "version": "1.0.0",
  "description": "collection javascript algorithms",
  "main": "framework.js",
  "scripts": {
	"test": "nyc mocha **/*.test.js",
	"coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
	"check-coverage": false,
	"all": true,
	"extension": [
	  ".js"
	],
	"include": [
	  "**/*.js"
	],
	"exclude": [
	  "docs/",
	  "**/*.test.js"
	],
	"reporter": [
	  "html",
	  "lcov",
	  "text",
	  "text-summary"
	],
	"report-dir": "docs/reports/coverage"
  },
  "repository": {
	"type": "git",
	"url": "https://github.com/shivajivarma/codebase-js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
	"url": "https://github.com/shivajivarma/codebase-js/issues"
  },
  "homepage": "https://github.com/shivajivarma/codebase-js#readme",
  "devDependencies": {
	"chai": "^4.1.2",
	"mocha": "^5.2.0"
  }
}
